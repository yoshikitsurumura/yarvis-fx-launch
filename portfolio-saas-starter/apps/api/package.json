    "test": "vitest run",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "express": "^4.19.2",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@pkg/types": "*"
  },
  "devDependencies": {
    "typescript": "^5.5.4",
    "ts-node": "^10.9.2",
    "vitest": "^2.0.5",
    "supertest": "^7.0.0",
    "@types/supertest": "^2.0.16",
    "@types/compression": "^1.7.5"
  }
}
